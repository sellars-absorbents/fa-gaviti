    {
      "name": "Python: Prod Environment",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env.prod",
      "justMyCode": false
    }
  ]
}